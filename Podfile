target 'Lez' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Lez
  pod 'Firebase/Core'
  pod 'Firebase/Firestore'
  pod 'FirebaseUI/Auth'
  pod 'FirebaseUI/Facebook'
  pod 'Firebase/Storage'
  
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'
  pod 'GoogleMaps'
  
  pod 'SnapKit', '~> 4.0.0'
  pod 'KeyboardHelper', '~> 1.0'
  pod 'PromiseKit', '~> 4.4'
  pod 'CodableFirebase'
  pod 'Koloda', '~> 4.3.1'
  
  pod 'AlamofireImage', '~> 3.3'
  pod 'Alamofire', '~> 4.5'
  
  target 'LezTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'LezUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
